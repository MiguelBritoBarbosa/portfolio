{
  "kind": "collectionType",
  "collectionName": "projetos",
  "info": {
    "singularName": "projeto",
    "pluralName": "projetos",
    "displayName": "Projeto",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "titulo": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "descricao": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "blocks",
      "required": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "tipoProjeto": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Profissional",
        "Pessoal",
        "Estudo"
      ],
      "required": true
    },
    "autores": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::autor.autor",
      "mappedBy": "projetos"
    },
    "destaque": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "required": true,
      "targetField": "titulo"
    },
    "link": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "repositorio": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "visibilidade": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Privado",
        "Público"
      ],
      "default": "Público"
    },
    "tecnologias": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tecnologia.tecnologia",
      "inversedBy": "projetos"
    },
    "bancoDeDados": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::banco-de-dados.banco-de-dados",
      "inversedBy": "projetos"
    },
    "experiencia": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::experiencia.experiencia",
      "inversedBy": "projetos"
    }
  }
}
